<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mattjoslenproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_student"
        android:label="Student Overview"
        android:roundIcon="@mipmap/ic_launcher_student_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AddTerm">
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".AddCourse">
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".AddAssessment">
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".EditCourse">
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".EditAssessment">
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver
            android:name=".MyReceiver"
            android:enabled="true"
            android:exported="true" />
    </application>

</manifest>
